@{
ViewData["Title"] = "PIM VIII";
}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title></title>
  </head>
  <header>
  </header>
  <body class="body">

    <form id="inserir_form">

      <div class="row-box">
        <strong>Pessoa</strong>
        <div>
          <label for="nome">Nome:</label>
          <input type="text" id= "nome"  name="nome"/>
        </div>
        <div>
          <label for="CPF">CPF:</label>
          <input type="text" name= "cpf" />
        </div>
        <div>
        </div>
      </div>
      <div class="column-box">
        <strong> Endereço </strong>

        <div class="row-box">
          <div>
            <label for="logradouro">logradouro:</label>
            <input type="text" name= "logradouro" />
          </div>
          <div>
            <label>Numero:</label>
            <input type="number" name="numero" />
          </div>
          <div>
            <label>CEP:</label>
            <input type="text" name="cep" />
          </div>
        </div>
        <div class="row-box">
          <div>
            <label>Bairro: </label>
            <input type="text" name="bairro" />
          </div>
          <div>
            <label>Cidade: </label>
            <input type="text" name="cidade" />
          </div>
          <div>
            <label>Estado: </label>
            <input type="text" name="estado" />
          </div>
        </div>
      </div>
      <div class="row-box">
        <div class="row-box">
          <strong>Telefone</strong>
          <button type="button" id="adicionar_telefones">+</button>
        </div>
        <div id="insira_telefones">
          <div id="telefone">
            <div>
              <label>Numero</label>
              <input type="number" name="t_numero"/>
            </div>
            <div>
              <label>DDD</label>
              <input type="number" name="t_ddd"/>
            </div>
            <div>
              <label>Tipo</label>
              <input type="text" name="t_tipo"/>
            </div>
          </div>
        </div>
      </div>
      <div>
        <input type="submit" value="Cadastrar" />
      </div>
    </form>
  </body>
</html>

